{
  "name": "tiny-code-react-antd-app-antd-app",
  "private": true,
  "version": "0.0.1",
  "description": "tiny-code-react-antd-app-antd-app",
  "type": "module",
  "keywords": [
    "tiny-code"
  ],
  "author": "jsjzh <kimimi_king@163.com>",
  "license": "MIT",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "vite --host localhost --port 8080 --open --force",
    "build": "npm run build:dev",
    "build:dev": "tsc && vite build --mode development",
    "build:pre": "tsc && vite build --mode preview",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "format": "prettier --write --cache --parser typescript \"**/*.[tj]s?(x)\"",
    "format:check": "prettier --check --cache --parser typescript \"**/*.[tj]s?(x)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsjzh/tiny-code-react-antd-app-antd-app.git"
  },
  "bugs": {
    "url": "https://github.com/jsjzh/tiny-code-react-antd-app-antd-app/issues"
  },
  "homepage": "https://github.com/jsjzh/tiny-code-react-antd-app-antd-app#readme",
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "antd": "^5.7.3",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "immer": "^9.0.21",
    "jsonp": "^0.2.1",
    "query-string": "^8.1.0",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "styled-components": "^5.3.11",
    "swr": "^2.2.0",
    "use-immer": "^0.8.1",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@types/jsonp": "^0.2.1",
    "@types/node": "^18.17.1",
    "@types/ramda": "^0.28.25",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.46.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-plugin-react": "^7.33.1",
    "typescript": "^4.9.5",
    "vite": "^4.4.7"
  }
}
